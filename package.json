{
  "name": "hydro-ui",
  "description": "",
  "version": "0.0.4",
  "author": "Phat Vo",
  "license": "ISC",
  "keywords": [
    "react"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsup",
    "dev": "storybook dev -p 3000"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "treeshake": true,
    "sourcemap": "inline",
    "minify": true,
    "clean": true,
    "dts": true,
    "splitting": false,
    "format": [
      "cjs",
      "esm"
    ],
    "external": [
      "react"
    ],
    "injectStyle": false
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/react": "^7.1.1",
    "@storybook/react-webpack5": "^7.1.1",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "storybook": "^7.1.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  }
}
